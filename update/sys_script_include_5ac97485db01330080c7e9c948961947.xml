        <name>RetrieveEmailAddresss</name>
        <script><![CDATA[var RetrieveEmailAddresss = Class.create();
RetrieveEmailAddresss.prototype = Object.extendsObject(global.AbstractAjaxProcessor, {
	
	getEmail: function(){
		var gr = new GlideRecord("sys_user");
		gr.addQuery("sys_id",this.getParameter('sysparm_user'));
		gr.query();
		gr.next();
		return gr.email+'';
	},
	
		type: 'RetrieveEmailAddresss'
	});]]></script>
